@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<form asp-action="CreateFull" enctype="multipart/form-data">

    <input name="name" placeholder="name"/>
    <input name="price" placeholder="price"/>
    <br />
    <input name="maker.name" placeholder="Maker Name"/>
    <input name="maker.country" placeholder="Maker Country"/>
    <br />
    <textarea name="Note" cols="10" rows="3" placeholder="note"></textarea>

    <input type="file" name="file" />


    <button type="submit">Send</button>
</form>
